{
    "manifest_version": 2,
    "name": "Twitch Live List",
    "description": "Lists live channels the user currently follows.",
    "version": "1.0.0",
    "icons": {"128": "media/icon_128.png"},
    "browser_action": {

        "default_icon": "media/icon.png",
        "default_popup": "popup.html"

    },
    "permissions": ["activeTab"]
}