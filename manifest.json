{
    "manifest_version": 2,
    "name": "Live On Twitch",
    "description": "Lists live channels the user currently follows.",
    "version": "1.0.0",
    "icons": {
        "128": "media/icon_128.png",
        "64": "media/icon_64.png",
        "16": "media/icon_16.png"
    },
    "background": {
        "scripts": [
            "js/jquery-3.4.1.min.js",
            "js/background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "alarms"
    ],
    "options_page": "options.html"
}